# Copy this file to .env and fill as needed. Values are optional unless marked REQUIRED.
# App settings
APP_ENV=dev

# Optional AWS S3 upload (set these only if you want to upload to S3)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=ap-south-1
S3_BUCKET_NAME=
S3_PREFIX=cloud-weather-data/

# Data settings
DATA_DIR=./data
REPORTS_DIR=./reports
CONFIG_FILE=./config/locations.yaml
DEFAULT_START_DATE=2024-10-01
DEFAULT_END_DATE=2024-10-07
